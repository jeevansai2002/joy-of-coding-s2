#include<stdio.h>
#include<stdlib.h>
int main(){
	int n,m,i,j,temp;
	printf("enter the no.of rows and columns in the matrix");
	scanf("%d %d",&n,&m);
	
	int a[n+m][m+n];
	printf("enter the array elements : \n");
	for(i=0;i<n;i++){
		for(j=0;j<m;j++){
			scanf("%d",&a[i][j]);
		
		}
	
	
	}
	for(i=0;i<n;i++){
		for(j=0;j<m;j++){
			if(i!=j){
				temp=a[i][j];
				a[i][j]=a[j][i];
				a[j][i]=temp;
			}
			
		
		
		}
	
	
	}
	printf("final matrix is :\n");
	for(i=0;i<m;i++){
		for(j=0;j<n;j++){
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	
	
	}
	

}
