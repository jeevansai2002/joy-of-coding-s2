#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(){
	char a[50];
	int count,max=0;
	char max_var;
	
	printf("enter a string");
	
	
	scanf("%[^\n]s",a);
	for(int i=0;a[i]!='\0';i++){
	
		count=0;
		if(a[i]=='1'){
			continue;
		}
		
		for(int j=i;a[j]!='\0';j++){
			if(a[j]==a[i]){
				count++;
			}
			
		}
		if(max<count){
			max=count;
			max_var=a[i];
		}
	
	}
	printf("maximum repeated character is %c with count %d",max_var,max);
		
		
		
}
